package bdd;

public class Salle {
	private String Etat_porte;
	private String Etat_fenetre;
	private String Etat_chauffage;
	private String Etat_lumiere;
	private int Temperature_in;
	private int ID;
	
	public String getEtat_porte() {
		return Etat_porte;
	}
	public void setEtat_porte(String etat_porte) {
		Etat_porte = etat_porte;
	}
	public String getEtat_fenetre() {
		return Etat_fenetre;
	}
	public void setEtat_fenetre(String etat_fenetre) {
		Etat_fenetre = etat_fenetre;
	}
	public String getEtat_chauffage() {
		return Etat_chauffage;
	}
	public void setEtat_chauffage(String etat_chauffage) {
		Etat_chauffage = etat_chauffage;
	}
	public String getEtat_lumiere() {
		return Etat_lumiere;
	}
	public void setEtat_lumiere(String etat_lumiere) {
		Etat_lumiere = etat_lumiere;
	}
	public int getTemperature_in() {
		return Temperature_in;
	}
	public void setTemperature_in(int temperature_in) {
		Temperature_in = temperature_in;
	}
	public int getID() {
		return ID;
	}
	public void setID(int iD) {
		ID = iD;
	}
	
	
}
