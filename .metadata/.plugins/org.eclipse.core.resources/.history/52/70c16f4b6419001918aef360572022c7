package bdd;

import java.util.List;

public class batiment {
	private int Etat_alarme;
	private int Temperature_out;
	private List<Salle> Salles;
	
	public static int getEtat_alarme() {
		return Etat_alarme;
	}
	public static void setEtat_alarme(int etat_alarme) {
		Etat_alarme = etat_alarme;
	}
	public static int getTemperature_out() {
		return Temperature_out;
	}
	public static void setTemperature_out(int temperature_out) {
		Temperature_out = temperature_out;
	}
	public Salle getSalles(int x) {
		Salle salle = Salles.get(x);
		return salle;
	}
	public void setSalles(List<Salle> salles) {
		Salles = salles;
	}
	
}
