package bdd;

public class Salle {
	private int Etat_porte;
	private int Etat_fenetre;
	private int Etat_chauffage;
	private int Etat_lumiere;
	private int Temperature_in;
	
	public static int getEtat_porte() {
		return Etat_porte;
	}
	public static void setEtat_porte(int etat_porte) {
		Etat_porte = etat_porte;
	}
	public static int getEtat_fenetre() {
		return Etat_fenetre;
	}
	public static void setEtat_fenetre(int etat_fenetre) {
		Etat_fenetre = etat_fenetre;
	}
	public static int getEtat_chauffage() {
		return Etat_chauffage;
	}
	public static void setEtat_chauffage(int etat_chauffage) {
		Etat_chauffage = etat_chauffage;
	}
	public static int getEtat_lumiere() {
		return Etat_lumiere;
	}
	public static void setEtat_lumiere(int etat_lumiere) {
		Etat_lumiere = etat_lumiere;
	}
	public static int getTemperature_in() {
		return Temperature_in;
	}
	public static void setTemperature_in(int temperature_in) {
		Temperature_in = temperature_in;
	}

	
}
